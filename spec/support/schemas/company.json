{
    "type": "object",
    "title": "The Root Schema",
    "required": [
      "id",
      "name",
      "city",
      "state",
      "founded_on",
      "description",
      "founders"
    ],
    "properties": {
      "id": {
        "type": "integer",
        "title": "The Id Schema",
        "default": 0,
        "examples": [
          1
        ]
      },
      "name": {
        "type": "string",
        "title": "The Name Schema",
        "default": "",
        "examples": [
          "Cybrilla Technologies"
        ]
      },
      "city": {
        "type": "string",
        "title": "The City Schema",
        "default": "",
        "examples": [
          "Bangalore"
        ]
      },
      "state": {
        "type": "string",
        "title": "The State Schema",
        "default": "",
        "examples": [
          "Karnataka"
        ]
      },
      "founded_on": {
        "type": "string",
        "title": "The Founded_on Schema",
        "default": "",
        "examples": [
          "2011-12-11"
        ]
      },
      "description": {
        "type": "string",
        "title": "The Description Schema",
        "default": "",
        "examples": [
          "Fintech Company"
        ]
      },
      "founders": {
        "type": "array",
        "title": "The Founders Schema",
        "items": {
          "type": "object",
          "title": "The Items Schema",
          "required": [
            "name",
            "title"
          ],
          "properties": {
            "name": {
              "type": "string",
              "title": "The Name Schema",
              "default": "",
              "examples": [
                "xhghgz"
              ]
            },
            "title": {
              "type": "string",
              "title": "The Title Schema",
              "default": "",
              "examples": [
                "xzx"
              ]
            }
          }
        }
      }
    }
  }